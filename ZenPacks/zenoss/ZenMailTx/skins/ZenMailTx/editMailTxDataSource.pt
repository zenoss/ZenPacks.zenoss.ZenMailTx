<tal:block metal:use-macro="here/templates/macros/page2">
<tal:block metal:fill-slot="contentPane"
    tal:define="editable python:here.isManager">

<form method="POST" enctype="multipart/form-data"
    tal:attributes="action string:${here/absolute_url_path}">
    <input type="hidden" name="zenScreenName" 
        tal:attributes="value template/id" /> 


        <tal:block tal:define="message request/message | string:State at time:; 
            tabletitle string:${message} ${here/getNowString}">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    <tr>
        <td class="tableheader">Name</td>
        <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="newId" size="40"
                tal:attributes="value here/id" />
        </td>
        <td class="tablevalues" tal:condition="not:editable"
            tal:content="here/id"/>
    </tr>
    <tr>
      <td class="tableheader">Source Type</td>
	  <td class="tablevalues" tal:content="here/sourcetype"></td>
    </tr>
    <tr>
        <td class="tableheader">Enabled</td>
        <td class="tablevalues" tal:condition="editable">
        <select class="tablevalues" name="enabled:boolean">
            <option tal:repeat="e python:(True,False)" tal:content="e"
                    tal:attributes="value e; selected python:e==here.enabled"/>
        </select>
        </td>
        <td class="tablevalues" tal:condition="not:editable" 
            tal:content="here/enabled"/>
    </tr>

    <tr>
            <td class="tableheader">Component</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="component" size="50"
                tal:attributes="value here/component" />
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/component"/>
        </tr>
        <tr>
            <td class="tableheader">Event Class</td>
            <td class="tablevalues" tal:condition="editable">
            <select class="tablevalues" name="eventClass">
                <option tal:repeat="evtcls here/getEventClassNames" 
                        tal:content="evtcls"
                        tal:attributes="selected python:evtcls==here.eventClass"/>
            </select>
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/eventClass"/>
        </tr>
        <tr>
            <td class="tableheader">Event Key</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="eventKey" size="50"
                tal:attributes="value here/eventKey" />
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/eventKey"/>
        </tr>
        <tr>
            <td class="tableheader">Severity</td>
            <td class="tablevalues" tal:condition="editable">
            <select class="tablevalues" name="severity:int">
               <option tal:repeat="sev here/ZenEventManager/getSeverities"
                       tal:content="python:sev[0]"
                       tal:attributes="selected python:sev[1]==here.severity; value python:sev[1]"/>
            </select>
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/getSeverityString"/>
        </tr>
        <tr>
            <td class="tableheader">Timeout (seconds)</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="timeout:int" size="10"
                tal:attributes="value here/timeout" />
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/timeout"/>
        </tr>
        <tr>
            <td class="tableheader">Cycle Time (seconds)</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="cycletime:int" size="10"
                tal:attributes="value here/cycletime" />
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/cycletime"/>
        </tr>
        <tr>
            <td class="tableheader">To Address</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="toAddress:string" size="30"
                tal:attributes="value here/toAddress" />
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/toAddress"/>
        </tr>
        <tr>
            <td class="tableheader">From Address</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="fromAddress:string" size="30"
                tal:attributes="value here/fromAddress" />
            </td>
          <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/fromAddress"/>
        </tr>



        <tr><td class="tableheader" colspan="3">&nbsp;</td></tr>
        <tr>
            <td class="tableheader">SMTP Host</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="smtpHost:string" size="50"
                tal:attributes="value here/smtpHost" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/smtpHost"/>
        </tr>
        <tr>
            <td class="tableheader">SMTP Username</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="smtpUsername:string" size="50"
                tal:attributes="value here/smtpUsername" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/smtpUsername"/>
        </tr>
        <tr>
            <td class="tableheader">SMTP Password</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" autocomplete="off" type="password" name="smtpPassword:string" size="50"
                tal:attributes="value here/smtpPassword" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="python:'*' * len(here.smtpPassword)"/>
        </tr>
	<tr>
	    <td class="tableheader">SMTP Transport Security</td>
	    <td class="tablevalues" tal:condition="editable">
	    <select class="tablevalues" name="smtpAuth:string">
		<option tal:repeat="e python:here.AuthModes" tal:content="e"
			tal:attributes="value e; selected python:e==here.smtpAuth"/>
	    </select>
	    </td>
	    <td class="tablevalues" tal:condition="not:editable" 
		tal:content="here/smtpAuth"/>
	</tr>
        <tr>
            <td class="tableheader">SMTP Port</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="smtpPort:int" size="10"
                tal:attributes="value here/smtpPort" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/smtpPort"/>
        </tr>
        <tr><td class="tableheader" colspan="3">&nbsp;</td></tr>
        <tr>
            <td class="tableheader">POP Host</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="popHost:string" size="50"
                tal:attributes="value here/popHost" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/popHost"/>
        </tr>
        <tr>
            <td class="tableheader">POP Username</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="popUsername:string" size="50"
                tal:attributes="value here/popUsername" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/popUsername"/>
        </tr>
        <tr>
            <td class="tableheader">POP Password</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" autocomplete="off" type="password" name="popPassword:string" size="50"
                tal:attributes="value here/popPassword" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="python:'*' * len(here.popPassword)"/>
        </tr>
	<tr>
	    <td class="tableheader">POP Transport Security</td>
	    <td class="tablevalues" tal:condition="editable">
	    <select class="tablevalues" name="popAuth:string">
		<option tal:repeat="e python:here.AuthModes" tal:content="e"
			tal:attributes="value e; selected python:e==here.popAuth"/>
	    </select>
	    </td>
	    <td class="tablevalues" tal:condition="not:editable" 
		tal:content="here/popAuth"/>
	</tr>
        <tr>
            <td class="tableheader">POP Port</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="popPort:int" size="10"
                tal:attributes="value here/popPort" />
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="here/popPort"/>
        </tr>
        <tr>
            <td class="tableheader">Allow Insecure Logins?</td>
            <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="checkbox" name="popAllowInsecureLogin:boolean" tal:attributes="checked here/popAllowInsecureLogin" />
            <input type="hidden" name="popAllowInsecureLogin:default:boolean"/>
            </td>
            <td class="tablevalues" tal:condition="not:editable" 
                tal:content="python:str(bool(here.popAllowInsecureLogins))"/>
        </tr>
        <tr><td class="tableheader" colspan="3">&nbsp;</td></tr>
        <tr>
            <td class="tableheader">Message Body</td>
            <td class="tablevalues" tal:condition="editable">
                <textarea class="tablevalues" rows=3 cols=60
                    name="messageBody:text"
                    tal:content="here/messageBody"/>
            </td>
            <td class="tablevalues" tal:condition="not:editable">
	        <pre tal:content="here/messageBody"/>
	    </td>
        </tr>

    <tr>
        <td class=tableheader align="left" >
        </td>
        <td align="left"  class="tableheader" colspan=3>
            <input class="tableheader" type="submit" value=" Save "
                name="zmanage_editProperties:method"/>
        </td>
    </tr>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>


<!----------------------------------------------------------------->

<form method="post" tal:attributes="action here/absolute_url_path">


        <tal:block tal:define="tableName string:rrdtempdplist; 
        objects here/datapoints;
        batch python:here.ZenTableManager.getBatch(tableName,objects);
		tabletitle string:DataPoints;
		menu_id string:DataPoint_list;">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    
    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'getId','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'rrdtype','Type')"/>
    </tr>
    <tr class="tablevalues" tal:repeat="dp batch">
        <td>
            <input tal:condition="editable" 
                type="checkbox" name="ids:list" 
                tal:attributes="value dp/id"/>
            <a tal:condition="editable"
                tal:attributes="href dp/getPrimaryUrlPath" 
                tal:content="dp/id">name</a>
            <tal:block tal:condition="not:editable" tal:content="dp/getId"/>
        </td>
        <td tal:content="dp/rrdtype"/>
    </tr>
    <tr>
        <td colspan="4" class="tableheader">
            <span metal:use-macro="here/zenTableNavigation/macros/navbody"/>
        </td>
    </tr>
    <tr class="tableheader" tal:condition="editable">
        <td colspan="4">
        <input class="tableheader" type="submit"
                name="SelectButton" value="Select All" 
                onClick="toggleSelect(this.form);return false"/>
        </td>
    </tr>
    <input type='hidden' name='tableName' tal:attributes="value tableName" />

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>

<!----------------------------------------------------------------->
</tal:block>
</tal:block>
